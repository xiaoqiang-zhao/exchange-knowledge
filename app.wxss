/**app.wxss**/
.container {
  width: 100%;
  min-height: 100vh;
  overflow: hidden;
  box-sizing: border-box;
  background: #f5f5f7;
}

section,
article,
header,
footer,
div {
  display: block;
  text-align: left;
}

/* ==== 公用样式 ==== */
/* 单行截断 */
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 多行截断 */
.line-clamp {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 3;
}

/* ==== 图标 ==== */
@font-face {
  font-family: "iconfont";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-handshake:before {
  content: "\e68a";
}

.icon-close:before {
  content: "\e6b0";
}

.icon-debug:before {
  content: "\e606";
}

.icon-camera:before {
  content: "\e666";
}

.icon-img:before {
  content: "\e6d6";
}

.icon-add:before {
  content: "\e600";
}

.icon-scene:before {
  content: "\e612";
}
